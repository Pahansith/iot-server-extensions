<?xml version="1.0" encoding="UTF-8"?>
<IOT>
	<Main>
		<Enroll>
			<Device-Class-Type> DB-DEVICE </Device-Class-Type>
			<User-Class-Type> DB-USER </User-Class-Type>
		</Enroll>

		<DeviceController>
			<DeviceDataStore> WSO2-BAM </DeviceDataStore>
			<DeviceControlQueue> AMQ </DeviceControlQueue>
		</DeviceController>
	</Main>

	<Device-Enroll-Endpoint>
		<class type="DB-DEVICE"> org.wso2.iot.enroll.db.DeviceManagementImpl </class>
	</Device-Enroll-Endpoint>

	<User-Enroll-Endpoint>
		<class type="DB-USER"> org.wso2.iot.enroll.db.UserManagementImpl </class>
	</User-Enroll-Endpoint>
	
	<DataStores>
		<DataStore>
			<class type="WSO2-BAM"> org.wso2.iot.devicecontroller.impl.BAMDataStore
			</class>
		</DataStore>

		<DataStore>
			<class type="WSO2-CEP"> org.wso2.iot.devicecontroller.impl.CEPDataStore
			</class>
		</DataStore>
	</DataStores>

	<ControlQueues>
		<ControlQueue>
			<class type="AMQ"> org.wso2.iot.devicecontroller.impl.AMQControlQueue
			</class>
		</ControlQueue>
	</ControlQueues>





</IOT>
